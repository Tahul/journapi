{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "url": "https://github.com/Tahul/laravel-keyable",
            "type": "git"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "givebutter/laravel-keyable": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "jamesmills/laravel-timezone": "^1.9",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^1.0",
        "shweshi/opengraph": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel-frontend-presets/tailwindcss": "^4.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear": [
            "rm -rf ./storage/temp/*",
            "php artisan cache:clear",
            "php artisan config:clear",
            "php artisan config:cache",
            "php artisan route:clear",
            "php artisan view:clear",
            "php artisan optimize:clear"
        ],
        "logs": [
            "Composer\\Config::disableProcessTimeout",
            "composer run reset-logs",
            "clear",
            "grc tail -f storage/logs/laravel.log"
        ],
        "reset-logs": [
            "rm -rf storage/logs/*",
            "touch storage/logs/laravel.log",
            "chmod 777 storage/logs/laravel.log"
        ]
    }
}
